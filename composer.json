{
    "name": "he426100/hyperf-ip",
    "type": "library",
    "description": "获取客户端ip，支持waf",
    "keywords": [
        "hyperf",
        "ip"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "he426100",
            "email": "mrpzx001@gmail.com"
        }
    ],
    "require": {
        "hyperf/config": "~3.0.0",
        "hyperf/http-server": "~3.0.0",
        "hyperf/utils": "~3.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "He426100\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "He426100\\ConfigProvider"
        }
    }
}
